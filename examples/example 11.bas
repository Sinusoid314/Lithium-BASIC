'EXAMPLE #11 - Test of SWAP command

dim dd(3) as number
var ss as number

showconsol

dd(1) = 1
ss = 9

print "Example #11 - SWAP Command Example"
print

print "Before:"
print "dd(1)  =  " & dd(1)
print "ss     =  " & ss
print

swap dd(1), ss

print "After:"
print "dd(1)  =  " & dd(1)
print "ss     =  " & ss
print